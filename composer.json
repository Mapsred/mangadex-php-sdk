{
    "name": "mapsred/mangadex-php-sdk",
    "description": "MangaDex PHP SDK",
    "license": "MIT",
    "authors": [{
        "name": "Maps_red"
    }],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "rector/rector": "^0.11.5",
        "symfony/finder": "^5.2"
    },
    "autoload": {
        "psr-4": { "Mapsred\\MangadexSDK\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Mapsred\\MangadexSDK\\Test\\" : "tests/" }
    }
}
